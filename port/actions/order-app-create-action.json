{
  "identifier": "create-order-app",
  "title": "Create an order app",
  "userInputs": {
    "properties": {
      "name": {
        "type": "string",
        "title": "Name",
        "description": "The name of the app"
      },
      "provider": {
        "type": "string",
        "title": "Provider",
        "default": "aws",
        "description": "The provider where the app is deployed",
        "enum": [
          "aws"
        ]
      },
      "region": {
        "type": "string",
        "title": "Region",
        "default": "us-east-1",
        "description": "The provider region",
        "enum": [
          "us-east-1",
          "us-east-2"
        ]
      }
    },
    "required": [
      "provider",
      "region"
    ]
  },
  "invocationMethod": {
    "type": "GITHUB",
    "org": "paulofponciano",
    "repo": "EKS-Crossplane-ArgoCD",
    "workflow": "create-order-app.yaml",
    "omitPayload": true
  },
  "trigger": "CREATE",
  "description": "Create a new order serverless app."
}
